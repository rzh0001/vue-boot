<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.productChannel.mapper.ProductChannelMapper">

    <insert id="batchSave">
      INSERT INTO pay_product_channel(product_code,channel_code,status,create_time) values
        <foreach collection="channelCodes" item="channel" separator=",">
            (#{productCode},#{channel},1,now())
        </foreach>
    </insert>
</mapper>