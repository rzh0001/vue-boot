<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.pay.mapper.ChannelBusinessEntityMapper">

    <select id="queryChannelBusiness" resultType="org.jeecg.modules.pay.entity.ChannelBusinessEntity" parameterType="String">
        SELECT
            *
        FROM
            sys_channel_businesss
        WHERE
            channel_code = #{channelCode}
        AND business_code = #{businessCode}
        AND del_flag IS NULL
    </select>
</mapper>