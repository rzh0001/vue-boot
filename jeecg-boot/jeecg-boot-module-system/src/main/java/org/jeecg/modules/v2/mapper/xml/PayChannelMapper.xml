<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.v2.mapper.PayChannelMapper">

    <select id="getChannlesByChannelCodes" parameterType="java.lang.String" resultType="org.jeecg.modules.v2.entity.PayChannel">
        select channel_name,channel_code,channel_gateway,channel_ip,channel_rate from pay_v2_channel where channel_code in
        <foreach item="item" collection="channelCodes" separator="," open="(" close=")" index="">
            #{item}
        </foreach>
        and status=1 and del_flag=0
    </select>
</mapper>